{
  "name": "tradersquare",
  "version": "1.0.0",
  "description": "Making stock analysis accessible to everyone!",
  "main": "index.js",
  "scripts": {
    "start": "node ./server/server.js",
    "dev": "nodemon ./server/server.js",
    "test": "mocha --compilers js:babel-core/register --require ./test/test_helper.js --recursive ./test",
    "testClient": "mocha -w test/helpers/browser.js test/*.spec.js",
    "test:watch": "npm run test -- --watch",
    "lint": "node ./node_modules/eslint/bin/eslint.js ./src",
    "dev:hot": "webpack-dev-server --hot --inline --progress --colors --watch --display-error-details --display-cached --content-base ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/traderquare/tradersquare.git"
  },
  "keywords": [
    "stock",
    "analysis",
    "market",
    "strategies"
  ],
  "author": "Angelina May, Cindy Sun, Chris Battenfield, Akul Aggarwal",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tradersquare/tradersquare/issues"
  },
  "homepage": "https://tradersquare.io",
  "engines": {
    "node": "6.6.0"
  },
